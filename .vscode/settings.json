// -*- jsonc -*-
{
  "clangd.arguments": ["--compile-commands-dir=/ros_ws/build_ros2"],
  "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
  "editor.formatOnSave": true,
